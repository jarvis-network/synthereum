{
  "version": 2,

  "projects": {
    "web3-utils": {
      "root": "libs/web3-utils",
      "sourceRoot": "libs/web3-utils/src/",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/web3-utils/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/web3-utils/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["yarn --cwd ./dist run build"],
            "parallel": false,
            "cwd": "./libs/web3-utils",
            "color": true
          }
        }
      }
    },
    "contracts": {
      "root": "libs/contracts",
      "sourceRoot": "libs/contracts/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/contracts/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/contracts/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["yarn run build"],
            "parallel": false,
            "cwd": "./libs/contracts",
            "color": true
          }
        },
        "build-solidity": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["yarn run build:solidity"],
            "parallel": false,
            "cwd": "./libs/contracts",
            "color": true
          }
        },
        "build-contracts-ts": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["yarn run build:contracts-ts"],
            "parallel": false,
            "cwd": "./libs/contracts",
            "color": true
          }
        }
      }
    },
    "validator": {
      "root": "apps/validator",
      "sourceRoot": "apps/validator/src",
      "projectType": "application",
      "prefix": "validator",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["yarn run build"],
            "parallel": false,
            "cwd": "./apps/validator",
            "color": true
          }
        },
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["yarn run start"],
            "parallel": false,
            "cwd": "./apps/validator",
            "color": true
          }
        },
        "serve-dev": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["yarn run start:dev"],
            "parallel": false,
            "cwd": "./apps/validator",
            "color": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/validator/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/validator/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
