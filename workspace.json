{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "frontend",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
