  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "runtimeArgs": ["-r", "ts-node/register"],
      "envFile": "${workspaceFolder}/packages/keeper-bot-v2/.env",
      "skipFiles": ["<node_internals>/**"],
      "args": [
        "${workspaceFolder}/packages/keeper-bot-v2/src/synfiat-keeper.ts"
      ],
      "env": {
        "TS_NODE_PROJECT": "${workspaceFolder}/packages/keeper-bot-v2/tsconfig.json"
      }
    }
  ]
}
