{
  "npmScope": "jarvis-network",
  "affected": { "defaultBase": "dev" },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["lint", "build"] }
    }
  },
  "projects": {
    "core-utils": { "tags": [] },
    "sol2ts-code-gen": { "tags": [] },
    "ui": { "tags": [] },
    "toolkit": { "tags": [], "implicitDependencies": ["ui", "contracts"] },
    "contracts": {
      "tags": [],
      "implicitDependencies": ["core-utils", "sol2ts-code-gen"]
    },
    "validator": { "tags": [], "implicitDependencies": ["validator-lib"] },
    "validator-lib": { "tags": [], "implicitDependencies": ["contracts"] },
    "frontend": {
      "tags": [],
      "implicitDependencies": ["ui", "toolkit", "contracts"]
    },
    "borrowing": {
      "tags": [],
      "implicitDependencies": ["ui", "toolkit", "contracts"]
    },
    "cli": { "tags": [], "implicitDependencies": ["contracts"] }
  },
  "workspaceLayout": { "appsDir": "apps", "libsDir": "libs" }
}
