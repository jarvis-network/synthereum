{
  "npmScope": "jarvis",
  "affected": {
    "defaultBase": "dev"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["lint", "build"]
      }
    }
  },
  "projects": {
    "web3-utils": {
      "tags": []
    },
    "contracts": {
      "tags": [],
      "implicitDependencies": ["web3-utils"]
    },
    "validator": {
      "tags": [],
      "implicitDependencies": ["contracts"]
    },
    "validator-lib": {
      "tags": [],
      "implicitDependencies": ["contracts"]
    },
    "meta-tx-lib": {
      "tags": [],
      "implicitDependencies": ["validator-lib"]
    },
    "frontend": {
      "tags": [],
      "implicitDependencies": ["contracts"]
    },
    "cli": {
      "tags": [],
      "implicitDependencies": ["contracts"]
	},
    "validator-meta-tx": {
      "tags": [],
 	  "implicitDependencies": ["meta-tx-lib"]
    }
  },
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "libs"
  }
}
