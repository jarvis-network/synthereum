{
  "npmScope": "jarvis-network",
  "affected": {
    "defaultBase": "dev"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["lint", "build"]
      }
    }
  },
  "projects": {
    "core-utils": {
      "tags": []
    },
    "crypto-utils": {
      "tags": []
    },
    "sol2ts-code-gen": {
      "tags": []
    },
    "ui": {
      "tags": []
    },
    "contracts": {
      "tags": [],
      "implicitDependencies": []
    },
    "synthereum-ts": {
      "tags": [],
      "implicitDependencies": ["contracts"]
    },
    "validator": {
      "tags": [],
      "implicitDependencies": []
    },
    "validator-lib": {
      "tags": [],
      "implicitDependencies": ["synthereum-ts"]
    },
    "toolkit": {
      "tags": [],
      "implicitDependencies": ["synthereum-ts"]
    },
    "frontend": {
      "tags": [],
      "implicitDependencies": ["toolkit"]
    },
    "borrowing": {
      "tags": [],
      "implicitDependencies": ["toolkit"]
    },
    "cli": {
      "tags": [],
      "implicitDependencies": ["synthereum-ts"]
    }
  },
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "libs"
  }
}
