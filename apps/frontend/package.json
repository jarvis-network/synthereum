{
  "name": "@jarvis-network/exchange-frontend",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "start:server": "next start",
    "build": "yarn prepare:assets && next build && next export",
    "build:withAnalyze": "yarn prepare:assets && ANALYZE=true next build && next export",
    "generate:graphql-query-types": "apollo codegen:generate --endpoint=https://api.thegraph.com/subgraphs/name/dimitarnestorov/synthereum-transactions --target typescript --excludes=node_modules/* --includes=**/*.ts --tagName=gql --outputFlat generated --passthroughCustomScalars --customScalarsPrefix TheGraph",
    "start:analyze": "cd .next/analyze && npx http-server -p 6543 -o",
    "start": "yarn prepare:assets && next dev",
    "prepare:assets": "cp -r ../../node_modules/@jarvis-network/ui/dist/assets/ public/ && cp -r ../../node_modules/@jarvis-network/ui/dist/assets/ public/assets/",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{ts,tsx}' --fix",
    "format": "prettier --check \"**/*.+(json|yml|yaml|md)\"",
    "format:fix": "prettier --write \"**/*.+(json|yml|yaml|md)\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@jarvis-network/app-toolkit": "*",
    "@jarvis-network/synthereum-ts": "*",
    "@jarvis-network/ui": "*",
    "@jarvis-network/core-utils": "*",
    "@next/bundle-analyzer": "^10.0.4",
    "@reduxjs/toolkit": "^1.4.0",
    "bnc-onboard": "^1.19.2",
    "color": "^3.1.3",
    "damlev": "^1.0.0",
    "fuse.js": "^6.4.6",
    "graphql-tag": "^2.11.0",
    "idb": "^6.0.0",
    "milliseconds": "^1.0.3",
    "next": "9.5.3",
    "next-pwa": "^5.0.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-infinite-scroll-component": "^6.0.0",
    "react-markdown": "^5.0.3",
    "react-redux": "^7.2.1",
    "recharts": "^2.0.4",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-observable": "^1.2.0",
    "ric-shim": "^1.0.0",
    "subscriptions-transport-ws": "^0.9.18",
    "use-memo-one": "^1.1.2",
    "web3": "^1.3.4"
  },
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "apollo": "^2.32.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "jest": "^26.6.3",
    "raw-loader": "^4.0.2",
    "typed-react-markdown": "^0.1.0"
  }
}
