{
  "name": "@jarvis-network/exchange-frontend",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "start:server": "next start",
    "build": "yarn prepare:assets && next build && next export",
    "build:withAnalyze": "yarn prepare:assets && ANALYZE=true next build && next export",
    "start:analyze": "cd .next/analyze && npx http-server -p 6543 -o",
    "start": "yarn prepare:assets && next dev",
    "prepare:assets": "cp -r ../../node_modules/@jarvis-network/ui/dist/assets/ public/",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{ts,tsx}' --fix",
    "format": "prettier --check \"**/*.+(json|yml|yaml|md)\"",
    "format:fix": "prettier --write \"**/*.+(json|yml|yaml|md)\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@jarvis-network/synthereum-contracts": "1.0.0",
    "@jarvis-network/ui": "^0.8.4",
    "@jarvis-network/web3-utils": "1.0.0",
    "@next/bundle-analyzer": "^10.0.4",
    "@reduxjs/toolkit": "^1.4.0",
    "bnc-onboard": "^1.18.0",
    "eth-ens-namehash": "^2.0.8",
    "ethereum-blockies": "^0.1.1",
    "idb-keyval": "^3.2.0",
    "lightweight-charts": "^3.1.5",
    "milliseconds": "^1.0.3",
    "next": "9.5.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-observable": "^1.2.0",
    "ric-shim": "^1.0.0",
    "web3": "^1.3.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "jest": "^26.6.3"
  }
}
