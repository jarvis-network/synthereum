{
  "name": "@jarvis-network/synthereum-contracts",
  "version": "1.0.0",
  "description": "",
  "directories": {
    "test": "test"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "clean": "git clean -fdx -e .env -e node_modules -- .",
    "format": "prettier --write './contracts/*.sol'",
    "build": "yarn build:scripts && yarn build:solidity && yarn build:typechain && yarn build:typechain-index && yarn build:tsc",
    "build:scripts": "tsc -p ./scripts/tsconfig.json",
    "build:solidity": "truffle compile && cp ./build/contracts/*.json ./src/contracts/abi",
    "build:typechain": "typechain --target web3-v1 --outDir ./src/contracts/typechain './build/contracts/*.json'",
    "build:typechain-index": "node ./dist/scripts/generate_typechain_header.js ./src/contracts",
    "build:tsc": "tsc -p ./tsconfig.json",
    "truffle:kovan": "truffle console --network kovan",
    "migrate:kovan": "truffle migrate --network kovan -f 1 --to 4 --skip-dry-run",
    "run:deposit": "truffle migrate --network kovan -f 6 --to 6 --skip-dry-run",
    "run:withdraw": "truffle migrate --network kovan -f 7 --to 7 --skip-dry-run",
    "run:add-assets": "truffle migrate --network kovan -f 5 --to 5 --skip-dry-run",
    "verify-contract:kovan": "truffle run verify TICFactory --network kovan"
  },
  "author": "Alessandro Bergamaschi",
  "license": "ISC",
  "dependencies": {
    "@jarvis-network/uma-core": "0.1.0",
    "@jarvis-network/web3-utils": "*",
    "@openzeppelin/contracts": "3.0.0",
    "@openzeppelin/test-helpers": "^0.5.9",
    "@truffle/hdwallet-provider": "^1.2.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@typechain/web3-v1": "^1.0.0",
    "ganache-cli": "^6.12.1",
    "ganache-time-traveler": "^1.0.15",
    "truffle": "5.1.49",
    "truffle-plugin-verify": "^0.4.0",
    "ts-node": "^9.0.0",
    "typechain": "^4.0.0"
  }
}
