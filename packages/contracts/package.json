{
  "name": "@jarvis/synthereum-contracts",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:solidity",
    "build:solidity": "truffle compile && mkdir -p dist/abi && cp ./build/contracts/*.json ./dist/abi/",
    "migrate:kovan": "truffle migrate --network kovan -f 1 --to 4 --skip-dry-run",
    "run:deposit": "truffle migrate --network kovan -f 6 --to 6 --skip-dry-run",
    "run:withdraw": "truffle migrate --network kovan -f 7 --to 7 --skip-dry-run",
    "run:add-assets": "truffle migrate --network kovan -f 5 --to 5 --skip-dry-run",
    "verify-contract:kovan": "truffle run verify TICHelper --network kovan && truffle run verify TICFactory --network kovan"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openzeppelin/contracts": "3.0.0-beta.0",
    "@openzeppelin/test-helpers": "^0.5.4",
    "@truffle/hdwallet-provider": "^1.0.34",
    "truffle-plugin-verify": "^0.4.0",
    "chainlink": "^0.8.2",
    "dotenv": "^8.2.0",
    "protocol": "github:UMAprotocol/protocol#8df987bee7aa90780b524c701c19db8b6d9d113b"
  },
  "devDependencies": {
    "ganache-time-traveler": "^1.0.14",
    "husky": "^4.2.3"
  }
}
