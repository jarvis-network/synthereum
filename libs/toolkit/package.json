{
  "name": "@jarvis-network/app-toolkit",
  "description": "Jarvis Network's App Toolkit",
  "homepage": "https://gitlab.com/jarvis-network/apps/exchange/mono-repo",
  "bugs": "https://gitlab.com/jarvis-network/apps/exchange/mono-repo/-/issues",
  "repository": {
    "url": "https://gitlab.com/jarvis-network/apps/exchange/mono-repo",
    "type": "git"
  },
  "keywords": [
    "react"
  ],
  "license": "MIT",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{ts,tsx}' --fix",
    "format": "prettier --check \"**/*.+(json|yml|yaml|md)\"",
    "format:fix": "prettier --write \"**/*.+(json|yml|yaml|md)\"",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --check"
    ]
  },
  "dependencies": {
    "bnc-onboard": "^1.19.2",
    "eth-ens-namehash": "^2.0.8",
    "ethereum-blockies": "^0.1.1",
    "web3": "^1.3.4"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-prettier-react": "^0.0.24",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.1.4",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  }
}
