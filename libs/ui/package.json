{
  "name": "@jarvis-network/ui",
  "description": "Jarvis Network's React.js UI components",
  "homepage": "https://gitlab.com/jarvis-network/core/market/ui",
  "bugs": "https://gitlab.com/jarvis-network/core/market/ui/-/issues",
  "repository": {
    "url": "https://gitlab.com/jarvis-network/core/market/ui",
    "type": "git"
  },
  "keywords": [
    "react",
    "react-component",
    "ui components",
    "typescript",
    "styled-components",
    "emotion"
  ],
  "license": "MIT",
  "version": "0.10.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "private": false,
  "scripts": {
    "start": "start-storybook -p 9009 -s public",
    "build": "yarn build:package && yarn build:storybook",
    "build:storybook": "build-storybook -s public",
    "build:package": "webpack --config webpack.build.config.js",
    "deploy:chromatic": "chromatic --storybook-build-dir storybook-static --exit-zero-on-changes",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{ts,tsx}' --fix",
    "format": "prettier --check \"**/*.+(json|yml|yaml|md)\"",
    "format:fix": "prettier --write \"**/*.+(json|yml|yaml|md)\"",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --check"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "framer-motion": "^2.0.0-beta.52",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.1.0",
    "react-table": "^6.8.6"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-backgrounds": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.150",
    "@types/node": "^14.14.34",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^3.1.2",
    "@types/react-table": "^6.8.7",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "chromatic": "^5.1.0",
    "css-loader": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-prettier-react": "^0.0.24",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.1.4",
    "polished": "^3.5.1",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react-sizeme": "^2.6.12",
    "source-map-loader": "^0.2.4",
    "storybook-react-router": "^1.0.8",
    "ts-jest": "^26.5.3",
    "ts-loader": "^8.0.18",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
